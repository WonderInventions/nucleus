{
  "name": "nucleus-server",
  "version": "1.1.3",
  "description": "Nucleus update server",
  "main": "lib/index.js",
  "bin": {
    "nucleus": "./lib/index.js"
  },
  "engines": {
    "node": ">=24.0.0"
  },
  "scripts": {
    "build:server": "tsc && rm -rf lib/__spec__ lib/**/__spec__",
    "build:fe:dev": "webpack --config webpack.config.js",
    "build:fe:prod": "webpack --config webpack.production.config.js",
    "clean": "rm -rf lib && rm -rf public_out",
    "commit": "git-cz",
    "dev": "concurrently --raw \"npm run start:fe\" \"npm run start:server:dev\" \"npm run start:static\"",
    "lint": "eslint --ext .ts,.tsx src public",
    "prepublishOnly": "npm run clean && npm run lint && npm run build:server && npm run build:fe:prod",
    "release:patch": "changelog -p && git add CHANGELOG.md && git commit -m \"updated CHANGELOG.md\" && npm version patch && git push && git push --tags",
    "release:minor": "changelog -m && git add CHANGELOG.md && git commit -m \"updated CHANGELOG.md\" && npm version minor && git push && git push --tags",
    "release:major": "changelog -M && git add CHANGELOG.md && git commit -m \"updated CHANGELOG.md\" && npm version major && git push && git push --tags",
    "start": "npm run start:server:prod",
    "start:fe": "webpack serve --progress",
    "start:server:dev": "nodemon --watch src --exec \"npm run build:server && cross-env DEBUG=nucleus* npm run start:server:prod\" -e ts",
    "start:server:prod": "node lib/index.js",
    "start:static": "make-dir .files && serve .files --port 9999 --silent",
    "test": "NODE_ENV=test tsx --test src/**/__spec__/*_spec.ts src/**/**/__spec__/*_spec.ts",
    "test:unit": "NODE_ENV=test tsx --test 'src/files/__spec__/*_spec.ts' 'src/files/**/__spec__/*_spec.ts'",
    "test:integration": "NODE_ENV=test tsx --test --test-force-exit src/__spec__/*_spec.ts"
  },
  "author": "Samuel Attard",
  "license": "Apache-2.0",
  "dependencies": {
    "@aws-sdk/client-cloudfront": "^3.983.0",
    "@aws-sdk/client-s3": "^3.983.0",
    "@aws-sdk/credential-providers": "^3.983.0",
    "body-parser": "^1.20.2",
    "child-process-promise": "^2.2.1",
    "colors": "^1.4.0",
    "compression": "^1.7.4",
    "debug": "^4.3.4",
    "express": "^5.2.1",
    "express-session": "^1.18.0",
    "is-png": "^1.1.0",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.9.0",
    "node-rsa": "^1.1.1",
    "passport": "^0.7.0",
    "passport-github": "^1.1.0",
    "pify": "^3.0.0",
    "reflect-metadata": "^0.1.10",
    "semver": "^7",
    "sequelize": "^6.37.0",
    "sequelize-typescript": "^2.1.6",
    "to-ico": "^1.1.5"
  },
  "devDependencies": {
    "@atlaskit/avatar": "^4.0.5",
    "@atlaskit/banner": "^6.1.2",
    "@atlaskit/button": "^5.4.0",
    "@atlaskit/css-reset": "^1.1.4",
    "@atlaskit/droplist": "^4.0.1",
    "@atlaskit/field-base": "^8.1.1",
    "@atlaskit/field-text": "^4.0.1",
    "@atlaskit/icon": "^6.6.0",
    "@atlaskit/modal-dialog": "^2.6.0",
    "@atlaskit/multi-select": "^7.1.3",
    "@atlaskit/spinner": "^2.2.3",
    "@atlaskit/tabs": "^4.0.0",
    "@atlaskit/tooltip": "^6.2.0",
    "@navjobs/upload": "^3.1.3",
    "@types/body-parser": "^1.19.5",
    "@types/compression": "^1.7.5",
    "@types/debug": "^4.1.12",
    "@types/express": "^5.0.6",
    "@types/express-session": "^1.17.10",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.0",
    "@types/passport": "^1.0.16",
    "@types/passport-github": "^1.1.12",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/semver": "^7.5.6",
    "@types/sinon": "^17.0.2",
    "@types/webpack-env": "^1.18.4",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "autoprefixer": "^10.4.17",
    "aws-sdk-client-mock": "^4.1.0",
    "commitizen": "^4.3.0",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "css-loader": "^6.8.1",
    "cz-customizable": "^7.0.0",
    "eslint": "^8.56.0",
    "favicons-webpack-plugin": "^6.0.1",
    "file-loader": "^6.2.0",
    "generate-changelog": "^1.8.0",
    "highlight.js": "^11.9.0",
    "html-webpack-plugin": "^5.6.0",
    "make-dir-cli": "^3.1.0",
    "mini-css-extract-plugin": "^2.7.6",
    "nodemon": "^3.0.2",
    "postcss": "^8.4.32",
    "postcss-loader": "^7.3.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.3",
    "react-router": "^6.21.0",
    "redux": "^4.2.1",
    "sass": "^1.69.5",
    "sass-loader": "^13.3.2",
    "serve": "^14.2.1",
    "sinon": "^17.0.1",
    "style-loader": "^3.3.3",
    "styled-components": "^6.1.2",
    "terser-webpack-plugin": "^5.3.9",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.3.3",
    "url-loader": "^4.1.1",
    "webpack": "^5.89.0",
    "webpack-bundle-analyzer": "^4.10.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "./.cz.js"
    }
  }
}
